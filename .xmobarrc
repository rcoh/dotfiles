Config { font = "xft:Deja Vu Sans Oblique Mono:size=12:antialias=true" 
, bgColor = "black"
, fgColor = "grey"
, overrideRedirect = False
, position = TopW L 95 
, lowerOnStart = True
, commands = [ Run Weather "KSFO" ["-t","SFO: <tempF>F","-L","45","-H","75","--normal","green","--high","red","--low","lightblue"] 36000
             , Run Network "wlp2s0" ["-L","0","-H","32","--normal","green","--high","red"] 10
             , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
             , Run Memory ["-t","Mem: <usedratio>%"] 10
             , Run Swap [ ] 10
             , Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
             , Run StdinReader
             , Run Com "spotifyinfo" [ ] "spotify-song" 10
             , Run Com "spotifyctl" [ "playstatus" ] "spotify-state" 10
             , Run Brightness ["-t", "ok", "--", "-D", "intel_backlight"] 50
	     , Run BatteryP ["BAT0"]
              ["-t", "<acstatus> <timeleft> (<left>%)",
               "-L", "10", "-H", "80", "-p", "3",
               "--", "-O", "<fc=green>Charging</fc> ", "-i", "",
               "-L", "-15", "-H", "-5", "-o", "",
               "-l", "red", "-m", "blue", "-h", "green"]
              600
             , Run CpuFreq ["-t", "Freq:<cpu0>", "-L", "0", "-H", "2",
                            "-l", "lightblue", "-n","white", "-h", "red"] 50
             ]
, sepChar = "%"
, alignSep = "}{"
, template = "%StdinReader% | %spotify-song% [%spotify-state%] }{ %battery% | %cpufreq% | %cpu% | %memory% | %wlp2s0% | %KSFO% | <fc=#ee9a00>%date%</fc>       "
}

